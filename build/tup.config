#######################
#                     #
# BUILD CONFIGURATION #
#                     #
#######################

CONFIG_IS_DEBUG=y

# Use clang address sanitizer
CONFIG_IS_ASAN=n

# Use clang memory sanitizer
CONFIG_IS_MSAN=n

# Note, you will need to have libtcmalloc available.
CONFIG_IS_TCMALLOC=n

# Create static executables
CONFIG_IS_STATIC=n
